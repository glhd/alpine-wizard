{
  "name": "@glhd/alpine-wizard",
  "version": "0.0.2",
  "description": "Multi-step wizard helpers for Alpine.js",
  "main": "dist/wizard.cjs.js",
  "module": "dist/wizard.esm.js",
  "unpkg": "dist/wizard.cdn.min.js",
  "exports": {
    ".": {
      "require": "./dist/wizard.cjs.js",
