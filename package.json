  "license": "MIT",
  "scripts": {
    "build": "node ./build.js",
    "watch": "node ./build.js --watch",
    "test": "cypress run"
  },
  "devDependencies": {
    "alpinejs": "^3.10.2",
    "cypress": "^10.1.0",
    "esbuild": "^0.14.43"
  },
  "dependencies": {
    "validatorjs": "^3.22.1"
  }
